{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "linterOptions": {
    "exclude": ["node_modules/**/*."]
  },
  "jsRules": {},
  "rules": {
    "no-console": false,
    "member-access": false,
    "object-literal-sort-keys": false,
    "ordered-imports": true,
    "interface-name": false,
    "quotemark": false,
    "strict-null-checks": false,
    "no-var-keyword": true
  },
  "rulesDirectory": []
}
